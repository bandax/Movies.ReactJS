    "start": "cross-env NODE_ENV=development  node ./src/server",
    "dev": "cross-env NODE_ENV=development node_modules/.bin/webpack-dev-server --mode development",
    "prod": "cross-env NODE_ENV=production node_modules/.bin/webpack-dev-server -p",
    "build:dev": "cross-env NODE_ENV=development node_modules/.bin/webpack",
    "build:prod": "cross-env NODE_ENV=production node_modules/.bin/webpack -p",
    "build-server": "node_modules/.bin/webpack --env.platform=server",
    "build-client": "node_modules/.bin/webpack --env.platform=web",
    "test": "jest",
    "test-coverage": "jest --coverage --collectCoverageFrom=src/**/*.{ts,tsx}",
    "test:watch": "jest --watch",
    "storybook": "start-storybook -p 9001",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src --color",
    "format": "prettier --write 'src/**/*.{ts,tsx,scss,css,json}'",
    "isready": "npm run format && npm run lint && npm run build"
  },
